<application>
  <component name="RemoteServers">
    <remote-server name="local" type="docker">
      <configuration>
        <option name="apiUrl" value="unix:///var/run/docker.sock" />
      </configuration>
    </remote-server>
    <remote-server name="do" type="docker">
      <configuration>
        <option name="apiUrl" value="unix:///tmp/docker.sock" />
        <option name="composeExecPath" value="$USER_HOME$/coding/qqdownload-tcp/docker-compose.override.yml" />
      </configuration>
    </remote-server>
    <remote-server name="rfc" type="docker">
      <configuration>
        <option name="apiUrl" value="http://rfc:2375" />
      </configuration>
    </remote-server>
  </component>
</application>